    "api-gateway",
    "compensation",
    "bam",
    "business-activity-monitoring",
    "zero-dependency"
  ],
  "license": "MIT",
  "author": "SOA One",
  "repository": {
    "type": "git",
    "url": "https://github.com/soa-one/soa-one.git",
    "directory": "packages/soa"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
