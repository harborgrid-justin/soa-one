      "types": "./dist/humantask.d.ts",
      "import": "./dist/humantask.js",
      "require": "./dist/humantask.js"
    },
    "./cep": {
      "types": "./dist/cep.d.ts",
      "import": "./dist/cep.js",
      "require": "./dist/cep.js"
    },
    "./b2b": {
      "types": "./dist/b2b.d.ts",
      "import": "./dist/b2b.js",
      "require": "./dist/b2b.js"
    },
    "./policy": {
      "types": "./dist/policy.d.ts",
      "import": "./dist/policy.js",
      "require": "./dist/policy.js"
    },
    "./mesh": {
      "types": "./dist/mesh.d.ts",
      "import": "./dist/mesh.js",
      "require": "./dist/mesh.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js",
      "require": "./dist/api.js"
